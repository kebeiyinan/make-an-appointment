	    	}
	    	.head {
				text-align: center;
				font-size: 20px;
				line-height: 50px;
			}
			.title{
				text-align: center;
				font-size: 16px;
				border: 1px solid red;
			}
			.title li{
				border-right: 1px solid red;
				box-sizing: border-box;
				height: 40px;
				line-height: 40px;
			}
			.news{
				height: 200px;
				overflow: hidden;
				font-size: 16px;
				border: 1px solid red;
			}
			.news li:first-child{
				border-top: none;
			}
			.news li{
				border: 1px solid red;
				border-top: none;
				text-align: center;
			}
			.news li p{
				border-right: 1px solid red;
				box-sizing: border-box;
				min-height: 40px;
				vertical-align: middle;
				padding-top: 5px;
			}
			.news li p:last-child{
				border-right:none;
			}
			.foot{
				width: 80%;
				margin: 0 auto;
			}
			.foot p {
				width: 25%;
				text-align: center;
				margin-top: 20px;
			}
			
